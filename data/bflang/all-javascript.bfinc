		<element pattern="toUpperCase"/>
	</group>
	<!-- elements that may contain a regexp -->
	<element pattern="replace" highlight="js-function" autocomplete="1" autocomplete_append="(,)" autocomplete_backup_cursor="2">
		<context id="c.js.regex" symbols="(" >
			<element pattern="(" highlight="js-brackets" >
				<context symbols=")" highlight="js-string">
					<element pattern=")" highlight="js-brackets" ends_context="2" />
				</context>
			</element>
		</context>
	</element>
		<element pattern="match">
			<context idref="c.js.regex" />
		</element>
		<element pattern="search">
			<context idref="c.js.regex" />
		</element>
	<!-- TODO highligt regex-string as js-value, comma asjs-brackets, newstring as js-string  -->
</group>
