			<element pattern="purple"/>
			<element pattern="red"/>
			<element pattern="silver"/>
			<element pattern="teal"/>
			<element pattern="white"/>
			<element pattern="yellow"/>
		</group>
		<element idref="css;" />
		<element idref="css}" />
	</context>
</element>
