		<pattern  name="with" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="xor" autocomplete="1" case_insens="1" style="keyword"/>
		
		<pattern  name="TRUE" autocomplete="1" case_insens="1" style="value"/>
		<pattern  name="FALSE" autocomplete="1" case_insens="1" style="value"/>
		<pattern  name="NOTHING" autocomplete="1" case_insens="1" style="value"/>
		
		<pattern name="&#34;" style="string">
			<context symbols="&#34;" style="string">
				<pattern name="&#34;" style="string" ends_context="1" />
			</context>
		</pattern>
		
		<pattern name="'" style="comment">
			<context symbols="%&gt;&#10;" style="comment">
				<pattern name="&#10;" ends_context="1" />
				<pattern name="%&gt;" ends_context="2" ends_block="1" blockstartpattern="&lt;%" style="region" mayfold="1" />
			</context>
		</pattern>
		
		<pattern name="for" case_insens="1" starts_block="1" mayfold="1" style="braces" />
		<pattern name="next" case_insens="1" ends_block="1" blockstartpattern="for" mayfold="1" style="braces" />
		<pattern name="do" case_insens="1" starts_block="1" mayfold="1" style="braces" />
		<pattern name="loop" case_insens="1" ends_block="1" blockstartpattern="do" mayfold="1" style="braces" />
		<pattern name="while" case_insens="1" starts_block="1" mayfold="1" style="braces" />
		<pattern name="wend" case_insens="1" ends_block="1" blockstartpattern="while" mayfold="1" style="braces" />
		<pattern name="exit[\ \t]+sub" is_regex="1" case_insens="1" style="keyword" />
		<pattern name="sub" case_insens="1" starts_block="1" mayfold="1" style="braces" />
		<pattern name="end[\ \t]+sub" is_regex="1" case_insens="1" ends_block="1" blockstartpattern="sub" mayfold="1" style="braces" />

		<pattern name="[0-9.]+" is_regex="1" style="value"/>
		<pattern name="exit[\ \t]+function" is_regex="1" case_insens="1" style="keyword" />
		<pattern name="function" starts_block="1" case_insens="1" mayfold="1" style="braces" />
		<pattern name="end[\ \t]+function" is_regex="1" case_insens="1" ends_block="1" blockstartpattern="function" mayfold="1" style="braces" />
		<pattern name="if" starts_block="1" is_regex="1" case_insens="1" mayfold="1" style="braces" />
		<pattern name="end[\ \t]+if" is_regex="1" case_insens="1" ends_block="1" blockstartpattern="if[\ \t]+.then" mayfold="1" style="braces" />
		<pattern name="with" starts_block="1" case_insens="1" mayfold="1" style="braces" />
		<pattern name="end[\ \t]+with" is_regex="1" case_insens="1" ends_block="1" blockstartpattern="with" mayfold="1" style="braces" />
		<pattern name="select" starts_block="1" case_insens="1" mayfold="1" style="braces" />
		<pattern name="end[\ \t]+select" is_regex="1" case_insens="1" ends_block="1" blockstartpattern="select" mayfold="1" style="braces" />

		<pattern name="%&gt;" ends_block="1" blockstartpattern="&lt;%" style="region" mayfold="1" ends_context="1" />
<!--
		<pattern name="[a-z][a-z0-9_]*[$%]?" is_regex="1" case_insens="1" style="variable"/>
-->
		<pattern name="[a-z][0-9._]*" is_regex="1" case_insens="1" style="variable"/>

	</context>
</pattern>


<!--
<pattern name="&lt;script.+javascript.+&gt;" is_regex="1" case_insens="1" starts_block="1" style="region">
	<context symbols=" ;(){}[]:\&#34;\\',&gt;&lt;*&amp;^%!+=-|/?#&#9;&#10;&#13;">
		<pattern name="&lt;/script&gt;" is_regex="1" case_insens="1" ends_block="1" blockstartpattern="&lt;script.+javascript.+&gt;" style="region" mayfold="1" ends_context="1" />
	</context>
</pattern>
-->

<pattern name="&lt;!--" style="comment" starts_block="1">
	<context symbols="-&gt; &#9;&#10;&#13;" style="comment">
		<pattern name="--&gt;" ends_block="1" blockstartpattern="&lt;!--" style="comment" mayfold="1" ends_context="1" />
	</context>
</pattern>

<pattern name="&amp;[a-z0-9#]+;" is_regex="1" style="value" />
</context>
</scanning>
