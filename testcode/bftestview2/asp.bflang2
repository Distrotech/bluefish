		<pattern  name="is" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="language" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="let" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="loop" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="mod" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="new" autocomplete="1" case_insens="1" style="keyword"/>
