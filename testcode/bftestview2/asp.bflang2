		<pattern  name="set" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="step" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="sub" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="then" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="to" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="until" autocomplete="1" case_insens="1" style="keyword"/>
		<pattern  name="wend" autocomplete="1" case_insens="1" style="keyword"/>
