@PACKAGE@
	command=@PACKAGE@ -n
	name=Bluefish @BF_APP_PACKAGE_ADDSTRING@
	can_open_multiple_files=true
	expects_uris=@BF_APP_EXPECT_URIS@
	uses_gnomevfs=@BF_APP_EXPECT_URIS@
	requires_terminal=false
	mime_types=text/html,text/plain,text/x-c,text/x-objc,text/x-c++,text/x-c-header,text/x-perl,text/x-java,text/xml,text/sgml,application/x-php,application/x-asp,text/vnd.wap.wml,image/svg,text/x-javascript,text/javascript,text/x-python,text/x-php,application/x-cgi,text/css,text/mathml,text/x-dtd,text/x-sql,text/x-po,text/*
	supported_uri_schemes=@BF_APP_SUPPORTED_URI_SCHEMES@

@PACKAGE@project
	command=@PACKAGE@ -p
	name=Bluefish project @BF_APP_PACKAGE_ADDSTRING@
	can_open_multiple_files=false
	expects_uris=false
	uses_gnomevfs=false
	requires_terminal=false
	mime_types=application/bluefish-project
