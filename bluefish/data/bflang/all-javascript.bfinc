<!-- These objects appear to be case insensitive -->
<group highlight="js-variable" autocomplete="1">
	<element pattern="global"/>
	<group class="javascript_navigator_object">
		<element pattern="navigator"/>
		<element pattern="mimeTypes"/>
		<element pattern="plugins"/>
	</group>
	<element pattern="screen" class="javascript_screen_object"/>
	<group class="javascript_window_object">
		<element pattern="window"/>
