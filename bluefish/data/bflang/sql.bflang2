<group autocomplete="1" case_insens="1" highlight="table">
	<element pattern="ALTER" />
	<element pattern="COLUMN" />
	<element pattern="CREATE" />
	<element pattern="FOREIGN" />
	<element pattern="INDEX" />
	<element pattern="KEY" />
